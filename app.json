{
  "expo": {
      "userInterfaceStyle": "automatic",
      "name": "Kalm Routines",
      "icon": "./assets/icon.png",
      "version": "1.1.0",
      "slug": "kalm-routine",
      "splash": {
          "image": "./assets/splash.png"
      },
      "ios": {
          "bundleIdentifier": "com.armincerf.kalmroutines",
          "buildNumber": "1.1.0",
          "supportsTablet": true,
          "infoPlist": {
              "UIBackgroundModes": [
                  "remote-notification"
              ]
          }
      },
      "android": {
          "package": "com.armincerf.kalm_routines",
          "versionCode": 3
      },
      "owner": "armincerf",
      "privacy": "public",
      "entryPoint": "./build/index.js",
      "platforms": [
          "ios",
          "android",
          "web"
      ],
      "orientation": "portrait",
      "updates": {
          "fallbackToCacheTimeout": 5000
      },
      "assetBundlePatterns": [
          "assets/images/*"
      ]
  }
}
